//IPLDEV   DD DISP=OLD,UNIT=3390,
//            VOL=(PRIVATE,RETAIN,SER=ZOS247)
//TRK0TEXT DD DSN=&TRK0TEXT,DISP=(OLD,DELETE)
//SYSIN    DD DSN=&DSFSYSIN,DISP=(OLD,DELETE)
//SYSPRINT DD SYSOUT=*
/*
//NOTOK   EXEC PGM=CPPMAXRC,COND=((0,GE,PUTIPL),(4000,LT))
